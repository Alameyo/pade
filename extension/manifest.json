{
  "name": "__MSG_manifest_extensionName__",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' 'unsafe-inline' data: http: https:",    
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "__MSG_manifest_extensionDescription__",
  "homepage_url": "https://igniterealtime.github.io/Pade/",
  "icons": {
      "128": "image.png",
      "16": "icon.png",
      "48": "image.png"
  },
  "default_locale": "en",
  "background": {
    "persistent": true,
    "page": "index.html"
  }, 
  "permissions": [
    "background", 
    "contextMenus",      
    "downloads",
    "storage",
    "idle",   
    "unlimitedStorage",    
    "notifications",
    "http://*/*", 
    "https://*/*"  
  ],
  "commands": {
    "activate_converse": {
        "suggested_key": {
          "default": "Ctrl+Shift+0"
        },
        "description": "Activate __MSG_manifest_extensionName__",
        "global": true
    },   
    "activate_settings": {
        "suggested_key": {
          "default": "Ctrl+Shift+9"
        },
        "description": "Activate __MSG_manifest_extensionName__ Settings",
        "global": true
    }    
  },  
  "options_ui": {
    "open_in_tab": true,  
    "page": "options/index.html"
  }, 
  "browser_action": {
    "default_icon": {
      "19": "icon.png",
      "38": "icon.png"
    }
  },
  "default_locale": "en"    
}


