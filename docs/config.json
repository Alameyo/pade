{
  "auto_login": false,
  "allow_logout": false,  
  "authentication": "login",
  "auto_away": 300,
  "domain_placeholder": "igniterealtime.org",
  "notification_icon": "./css/images/logo/conversejs-filled.svg",
  "view_mode": "fullscreen",
  "muc_show_join_leave_status": true,
  "play_sounds": true,
  "notify_all_room_messages": [],
  "muc_mention_autocomplete_min_chars": 0,
  "assets_path": "./dist/",
  "clear_messages_on_reconnection": true,
  "default_domain": "igniterealtime.org",
  "bosh_service_url": "https://igniterealtime.org/http-bind/",
  "auto_reconnect": true,
  "roster_groups": true,
  "singleton": false,
  "debug": false,
  "message_archiving": "always",
  "auto_focus": true,
  "i18n": "en",
  "show_message_load_animation": false,
  "allow_message_corrections": "all",
  "whitelisted_plugins": [
    "content", 
    "canned", 
    "screencast", 
    "vmsg", 
    "directory", 
    "search", 
    "jitsimeet", 
    "audioconf", 
    "fastpath",
    "pade"
  ],
  "message_carbons": true,
  "muc_fetch_members": false,
  "blacklisted_plugins": [
    "converse-minimize",
    "converse-dragresize"
  ], 
  "message_limit": 0,
  "sounds_path": "./dist/sounds/",
  "enable_smacks": false,
  "show_client_info": false,  
  "auto_join_rooms": ["open_chat@conference.igniterealtime.org"]
}